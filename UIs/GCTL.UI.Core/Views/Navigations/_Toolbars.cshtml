@model ToobarModel
<div class="row">
    <div class="col-md-5">
        <h6>@Model.Title</h6>
    </div>
    <div class="col-md-7">
        <div class="btn-toolbar d-flex justify-content-end">
            <div class="btn-group">
                <button type="button" class="btn btn-default js-navigation-save">
                    <i class="fa fa-save">&nbsp;</i> Save
                </button>
                <button type="button" class="btn btn-default js-navigation-delete-confirm">
                    <i class="far fa-minus-square">&nbsp;</i>
                    Delete
                </button>
                <button type="button" class="btn btn-default js-navigation-clear">
                    <i class="fas fa-recycle">&nbsp;</i>
                    Clear
                </button>
                <button type="button" class="btn btn-default js-orders">
                    <i class="fas fa-sort">&nbsp;</i>
                    Save Sort Order
                </button>
            </div>
        </div>     
    </div>
</div>

